+
+        // 获取响应的状态码
+        int statuscode = response.getStatusLine().getStatusCode();
+        System.out.println("statusCode = " + statuscode);
+
+        if (statuscode==200){
+            String result = EntityUtils.toString(response.getEntity());
+            System.out.println("=======" + result);
+        }
+
+    }
+}
